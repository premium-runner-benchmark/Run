    "name": "lumi-run",
    "description": "Lumi Run",
    "version": "1.0.0",
    "main": "build/src/server/boot/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lumieducation/Run.git"
    },
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "build:client": "node scripts/build.js",
        "build:client:docker": "npm run build:client && docker build -t lumieducation/run-client:latest --file docker/client/Dockerfile .",
        "build:server": "npx tsc --project tsconfig.server.json",
        "ci": "npm run lint && npm run format:check && npm run build:prod && NODE_ENV=test npm run test",
        "format": "prettier --write \"{src,test}/**/*.ts\"",
        "format:check": "prettier --check \"{src,test}/**/*.ts\"",
        "lint": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
        "test": "./node_modules/.bin/jest",
        "test:watch": "jest --watch",
        "uninstall": "rm -rf node_modules/ && rm -rf build/ && rm -rf h5p/ && rm -rf dist/",
        "update:api": "node scripts/api.js && npm run format",
        "sentry": "node scripts/sentry.js",
        "semantic-release": "semantic-release",
        "start": "node scripts/start.js",
        "heroku-postbuild": "npm run build:server"
    },
